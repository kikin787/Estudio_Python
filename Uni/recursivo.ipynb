   "source": [
    "def fact(n):\n",
    "    if(n == 0 or n == 1):\n",
    "        print(n)\n",
    "        return 1\n",
    "    else:\n",
    "        print(n)\n",
    "        return n * fact(n-1)\n",
    "r = fact(5)\n",
    "print(r)"
   ]
  }
